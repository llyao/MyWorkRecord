@using ClientCore.DB

@{
    ViewBag.Title = "Welcome";
}


@RenderPage("~/Views/Shared/_NavLeft.cshtml")

@{
    if (Session["user"] != null)
    {
        var user = (Session["user"] as UserTableData);
        <div style="text-align: center"><h2>你好啊， @user.Nick </h2></div>;
    }
}
<div>
    Project Details:<br />
    <div>
        module List  :
        <ul id="modulelist">
            @foreach (var module in ViewData["modules"] as List<string>)
            {
                <li>
                    @module
                </li>
            }
        </ul>
    </div>
</div>

